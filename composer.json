{
    "name": "helios-ag/fm-summernote-bundle",
    "description": "Summernote bundle, adds Summernote WYSIWYG editor to your Symfony2/3/4 project",
    "keywords": ["summernote","WYSIWYG", "editor"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/helios-ag/summernote-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
      {
        "name": "Al Ganiev",
        "email": "helios.ag@gmail.com"
      },
      {
        "name": "Symfony Community",
        "homepage": "https://github.com/helios-ag/summernote-bundle/contributors"
      }
    ],
    "require": {
      "php":                           "^5.6|^7.0",
      "symfony/framework-bundle":      "~2.8|~3.0|~4.0",
      "symfony/twig-bundle":           "~2.8|~3.0|~4.0"
    },
    "require-dev" : {
      "symfony/phpunit-bridge": "~2.8|~3.0|~4.0",
      "matthiasnoback/symfony-config-test":  "~2",
      "matthiasnoback/symfony-dependency-injection-test": "~1|~2",
      "symfony/finder": "~2.8|~3.0|~4.0",
      "satooshi/php-coveralls": "~1",
      "phpunit/phpunit": "~5"
    },
    "suggest": {
      "helios-ag/fm-elfinder-bundle": "Elfinder bundle, file manager"
    },
    "autoload": {
      "psr-4": { "FM\\SummernoteBundle\\": "" }
    },
    "extra": {
      "branch-alias": {
          "dev-master": "1.0-dev"
      }
    }
}
